{
  "name": "lolly",
  "productName": "lolly",
  "version": "1.0.0",
  "description": "League of Legends layout for timing summoner's spells",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": {
    "name": "Victor Martin Camarero",
    "email": "victormartincamarero@gmail.com"
  },
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "lolly"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "electron": "7.1.9",
    "tslint": "^5.20.1",
    "node": "12.14.1",
    "typescript": "^3.7.5",
    "@types/node": "12.12.6"
  }
}
